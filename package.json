      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.13",
    "@rescripts/rescript-env": "^0.0.11",
    "concurrently": "^5.0.2",
    "electron": "^7.1.9",
    "electron-builder": "^22.2.0",
    "electron-devtools-installer": "^2.2.4",
    "wait-on": "^4.0.0"
  }
